<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.serasa.crud_room">

    <application
        android:name=".CrudModule"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crud_room">
        <activity
            android:name=".view_activity.AppointmentActivity"
            android:exported="true" />
        <activity
            android:name=".view_activity.CategoryActivity"
            android:exported="true" />
        <activity
            android:name=".view_activity.DoctorActivity"
            android:exported="true" />
        <activity
            android:name=".view_activity.PatientActivity"
            android:exported="true" />
        <activity
            android:name=".view_activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>